{
  "expo": {
    "name": "Rescue Bag",
    "slug": "rescue-bag",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1F13"
    },
    "ios": {
